# RA-OCR API Keys Configuration
# Copy this file to .env and fill in your API keys

# Google AI API Key (required for Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (optional - for GPT-4o as Judge)
OPENAI_API_KEY=your_openai_api_key_here

# =============================================================================
# Dual Judge Configuration
# =============================================================================

# Enable dual-judge system (true/false)
USE_DUAL_JUDGE=true

# Use both Gemini and GPT-4o for cross-model consensus (requires OpenAI key)
USE_CROSS_MODEL=true

# Use dedicated equation specialist judge
USE_EQUATION_SPECIALIST=true

# Use verification gate before accepting high scores
USE_VERIFICATION=true

# Judge weights (for cross-model averaging)
GEMINI_WEIGHT=0.5
OPENAI_WEIGHT=0.5
EQUATION_WEIGHT=0.3
